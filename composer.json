{
    "name": "3dgoo/laravel-http-timeout-retry-provider",
    "description": "Http timeout retry provider for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Nowina-Krowicki",
            "email": "michael@3dgoo.com"
        }
    ],
    "keywords": [
        "laravel",
        "http",
        "timeout",
        "retry"
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3 || ^8.4",
        "illuminate/http": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.9",
        "laravel/pint": "^1.20",
        "nunomaduro/collision": "^7.12 || ^8.0",
        "orchestra/testbench": "^8.36 || ^9.0",
        "overtrue/phplint": "^9.6",
        "pestphp/pest": "^2.36 || ^3.0",
        "pestphp/pest-plugin-laravel": "^2.4 || ^3.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "X3dgoo\\HttpTimeoutRetryProvider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "X3dgoo\\HttpTimeoutRetryProvider\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "X3dgoo\\HttpTimeoutRetryProvider\\Providers\\HttpTimeoutRetryProvider"
            ]
        }
    }
}